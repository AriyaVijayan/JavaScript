
function vowelsAndConsonants(s) {
    var vowels = ['a','e','i','o','u']
    var consonants =''
    var vowelarray= []
    for(var idx =0; idx<s.length; idx++)
    {
          if (vowels.indexOf(s[idx]) !== -1) {
            console.log(s[idx]);
        } else {
            consonants += s[idx] + '\n'
        }
    }
    console.log(consonants);
    }
   


